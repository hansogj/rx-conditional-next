{
    "name": "rx-conditional-next",
    "version": "0.0.1",
    "description": "Update BehavioralSubject on  fulfilled conditions only",
    "main": "lib/index.js",
    "types": "lib/index.d.ts",
    "files": [
        "lib/**/*"
    ],
    "scripts": {
        "test": "karma start  karma.conf.js --single-run",
        "test:watch": "karma start  karma.conf.js --browsers=Chrome",
        "build": "tsc",
        "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
        "lint": "tslint -p tsconfig.json",
        "prepare": "npm run build",
        "prepublishOnly": "npm test && npm run lint",
        "preversion": "npm run lint",
        "version": "npm run format && git add -A src",
        "postversion": "git push && git push --tags"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/hansogj/rx-conditional-next.git"
    },
    "keywords": [
        "rxjs",
        "next",
        "behavoral",
        "subject"
    ],
    "author": "hansogj@gmail.com",
    "license": "ISC",
    "devDependencies": {
        "prettier": "^1.16.4",
        "tslint": "^5.13.1",
        "tslint-config-prettier": "^1.18.0",
        "typescript": "^3.3.3333",
        "@types/jasmine": "^2.8.2",
        "@types/node": "^8.0.0",
        "awesome-typescript-loader": "^3.4.1",
        "browserify": "^14.5.0",
        "jasmine-core": "^2.8.0",
        "karma": "^1.7.1",
        "karma-browserify": "^5.1.2",
        "karma-chrome-launcher": "^2.2.0",
        "karma-coverage": "^1.1.2",
        "karma-jasmine": "^1.1.0",
        "karma-typescript": "^3.0.8",
        "tsify": "^3.0.3"
    },
    "dependencies": {
        "rxjs": "6.3.3"
    }
}
